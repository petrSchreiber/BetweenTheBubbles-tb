function tbMain()
  dim test as easing
  dim fp(4) as points
  
  fp(1).x = 0
  fp(1).y = 0
  fp(2).x = 0
  fp(2).y = 0.5
  fp(3).x = 1
  fp(3).y = 0.5  
  fp(4).x = 1
  fp(4).y = 1
    
  
  if test.init(%Ease_beziercubic,,,,,fp) Then
    printL "init done"
    WaitKey
    for i as currency = 0 to 1 step 0.01
      printl test.ease(i)
    next
    waitkey
  else
    printl("fail")
    WaitKey
  endif
end Function
